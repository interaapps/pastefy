	Frame.Visible = false
	MiniTab.Position = Frame.Position
	MiniTab.Visible = true
end)
MiniTab.MouseButton1Click:Connect(function()
	Frame.Position = MiniTab.Position
	Frame.Visible = true
	MiniTab.Visible = false
end)

-- Capturar mensagens
local function ConnectPlayer(plr)
	plr.Chatted:Connect(function(message)
		AddMessage(plr.Name..": "..message)
		if string.find(message:lower(), "/w") or string.find(message:lower(), "whisper") then
			StarterGui:SetCore("SendNotification", {
				Title = "Chat Incomum üï¥Ô∏è",
				Text = "Mensagem sus encontrada",
				Duration = 5
			})
		end
	end)
end
for _, plr in ipairs(Players:GetPlayers()) do
	ConnectPlayer(plr)
end
Players.PlayerAdded:Connect(ConnectPlayer)

AddMessage("‚úÖ Chat J7 HUB üï¥Ô∏è iniciado!")
print("Hello World!")
end})

local Section = Tab2:AddSection({"Sharigam"})

Tab2:AddButton({"Sharigam", function(Value)
-- Servi√ßos
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer
local Remote = ReplicatedStorage.RE:FindFirstChild("1Gu1n")

-- Script principal fornecido
local index = 1

local function mainScript()
    RunService.Stepped:Connect(function()
        local allPlayers = Players:GetPlayers()
        if #allPlayers < 2 then return end

        index += 1
        if index > #allPlayers then index = 1 end

        local target = allPlayers[index]
        if target == LocalPlayer then
            index += 1
            if index > #allPlayers then index = 1 end
            target = allPlayers[index]
        end

        if target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
            local crazyVector = Vector3.new(
                math.random(1e14, 1e15),
                math.random(1e14, 1e15),
                math.random(1e14, 1e15)
            )

            local args = {
                [1] = target.Character.HumanoidRootPart,
                [2] = target.Character.HumanoidRootPart,
                [3] = crazyVector,
                [4] = target.Character.HumanoidRootPart.Position,
                [5] = LocalPlayer.Backpack:FindFirstChild("Assault") and LocalPlayer.Backpack.Assault.GunScript_Local:FindFirstChild("MuzzleEffect"),
                [6] = LocalPlayer.Backpack:FindFirstChild("Assault") and LocalPlayer.Backpack.Assault.GunScript_Local:FindFirstChild("HitEffect"),
                [7] = 0,
                [8] = 0,
                [9] = { [1] = false },
                [10] = {
                    [1] = 25,
                    [2] = Vector3.new(100, 100, 100),
                    [3] = BrickColor.new(29),
                    [4] = 0.25,
                    [5] = Enum.Material.SmoothPlastic,
                    [6] = 0.25
                },
                [11] = true,
                [12] = false
            }

            Remote:FireServer(unpack(args))
        end
    end)
end

-- Criar a interface gr√°fica com a imagem rotativa
local function createRotatingImage()
    -- Criar ScreenGui
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "RotatingImageGui"
    screenGui.Parent = LocalPlayer.PlayerGui
    screenGui.ResetOnSpawn = false

    -- Criar ImageLabel para a imagem
    local imageLabel = Instance.new("ImageLabel")
    imageLabel.Name = "IdiotImage"
    imageLabel.Size = UDim2.new(0, 200, 0, 200) -- Tamanho da imagem
    imageLabel.Position = UDim2.new(0.5, -100, 0.5, -100) -- Centralizado
    imageLabel.BackgroundTransparency = 1
    imageLabel.Image = "rbxassetid://139958375809767" -- ID da imagem
    imageLabel.Parent = screenGui

    -- Anima√ß√£o de rota√ß√£o cont√≠nua
    local rotationSpeed = 360 -- Graus por segundo
    RunService.RenderStepped:Connect(function(deltaTime)
        imageLabel.Rotation = (imageLabel.Rotation + rotationSpeed * deltaTime) % 360
    end)
end

-- Criar luz vermelha forte
local function setStrongRedLight()
    -- Aguardar at√© que o personagem do jogador local esteja carregado
    if not LocalPlayer.Character or not LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        LocalPlayer.CharacterAdded:Wait()
    end

    -- Criar PointLight no HumanoidRootPart
    local pointLight = Instance.new("PointLight")
    pointLight.Name = "StrongRedLight"
    pointLight.Color = Color3.fromRGB(255, 0, 0) -- Vermelho puro
    pointLight.Brightness = 3 -- Alta intensidade (ajust√°vel)
    pointLight.Range = 100 -- Grande alcance para iluminar o ambiente
    pointLight.Shadows = true -- Ativar sombras para maior realismo
    pointLight.Parent = LocalPlayer.Character.HumanoidRootPart
end

-- Executar as fun√ß√µes
mainScript()
createRotatingImage()
setStrongRedLight()
print("Sharigam ativado")
end})

local Section = Tab2:AddSection({"Player Character"})


local Players = game:GetService("Players")
local localPlayer = Players.LocalPlayer

local selectedPlayerName = nil
local headsitActive = false

local function headsitOnPlayer(targetPlayer)
    local character = localPlayer.Character or localPlayer.CharacterAdded:Wait()
    local humanoid = character:FindFirstChildOfClass("Humanoid")

    if not targetPlayer.Character or not targetPlayer.Character:FindFirstChild("Head") then
        warn("Jogador alvo sem cabe√ßa ou personagem.")
        return false
    end
    local targetHead = targetPlayer.Character.Head
    local localRoot = character:FindFirstChild("HumanoidRootPart")
    if not localRoot then
        warn("Seu personagem n√£o tem HumanoidRootPart.")
        return false
    end

    localRoot.CFrame = targetHead.CFrame * CFrame.new(0, 2.2, 0)

    for _, v in pairs(localRoot:GetChildren()) do
        if v:IsA("WeldConstraint") then
            v:Destroy()
        end
    end

    local weld = Instance.new("WeldConstraint")
    weld.Part0 = localRoot
    weld.Part1 = targetHead
    weld.Parent = localRoot

    if humanoid then
        humanoid.Sit = true
    end

    print("Headsit ativado em " .. targetPlayer.Name)
    return true
end

local function removeHeadsit()
    local character = localPlayer.Character or localPlayer.CharacterAdded:Wait()
    local humanoid = character:FindFirstChildOfClass("Humanoid")
    local localRoot = character:FindFirstChild("HumanoidRootPart")
    if localRoot then
        for _, v in pairs(localRoot:GetChildren()) do
            if v:IsA("WeldConstraint") then
                v:Destroy()
            end
        end
    end
    if humanoid then
        humanoid.Sit = false
    end

    print("Headsit desativado.")
end

-- Fun√ß√£o para encontrar jogador por nome parcial
local function findPlayerByPartialName(partial)
    partial = partial:lower()
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= localPlayer and player.Name:lower():sub(1, #partial) == partial then
            return player
        end
    end
    return nil
end

-- Notifica√ß√£o com imagem do jogador
local function notifyPlayerSelected(player)
    local StarterGui = game:GetService("StarterGui")
    local thumbType = Enum.ThumbnailType.HeadShot
    local thumbSize = Enum.ThumbnailSize.Size100x100
    local content, _ = Players:GetUserThumbnailAsync(player.UserId, thumbType, thumbSize)

    StarterGui:SetCore("SendNotification", {
        Title = "Player Selecionado",
        Text = player.Name .. " foi selecionado!",
        Icon = content,
        Duration = 5
    })
end

-- TextBox para digitar nome do player
Tab2:AddTextBox({
    Name = "Nome do Jogador",
    Description = "Digite parte do nome",
    PlaceholderText = "ex: lo ‚Üí Lolyta",
    Callback = function(Value)
        local foundPlayer = findPlayerByPartialName(Value)
        if foundPlayer then
            selectedPlayerName = foundPlayer.Name
            notifyPlayerSelected(foundPlayer)
        else
            warn("Nenhum jogador encontrado com esse nome.")
        end
    end
})

-- Bot√£o para ativar/desativar headsit
-- Bot√£o para ativar/desativar headsit (vers√£o simplificada)
Tab2:AddButton({"", function()
    if not selectedPlayerName then
    
        return
    end

    if not headsitActive then
        local target = Players:FindFirstChild(selectedPlayerName)
        if target and headsitOnPlayer(target) then
            headsitActive = true
        end
    else
        removeHeadsit()
        headsitActive = false
    end
end})




