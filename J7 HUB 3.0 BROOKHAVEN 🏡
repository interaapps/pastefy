        local input = Value:lower()
        if input ~= "" then
            for _, plr in ipairs(Players:GetPlayers()) do
                if plr.Name:lower():sub(1, #input) == input then
                    selectedPlayer = plr
                    showNotification(plr.Name, "rbxthumb://id=" .. plr.UserId .. "?width=50&height=50")
                    break
                end
            end
        else
            selectedPlayer = nil
            showNotification("Nenhum", nil)
        end
    end
})

